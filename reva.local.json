    "AWS_SECRET_ACCESS_KEY": "{{cred_AWS_SECRET_ACCESS_KEY}}",
    "AWS_SESSION_TOKEN": "{{cred_AWS_SESSION_TOKEN}}"
  },
  "servers": [
    {
      "name": "redis",
      "enable": true,
      "script": {
        "path": "~",
        "start": "redis-server --port 6379",
        "stop": "redis-cli -p 6379 shutdown"
      },
      "timeout": 2
    },
    {
      "name": "temporal",
      "enable": true,
      "script": {
        "path": "~",
        "start": "temporal server start-dev --db-filename ~/Projects/reva/data/temporal/temporal.db",
        "stop": "kill -9 $(lsof -t -i :8233)"
      },
      "timeout": 2
    },
    {
      "name": "dynamodb",
      "enable": true,
      "script": {
        "path": "~/Tools/dynamodb",
        "start": "java -Djava.library.path=./DynamoDBLocal_lib -jar DynamoDBLocal.jar -sharedDb -port 8005 -dbPath '.' config.db",
        "stop": "kill -9 $(lsof -t -i :8005)"
      },
      "timeout": 2
    },
    {
      "name": "mysql",
      "enable": true,
      "script": {
        "path": "~",
        "start": "mysql.server start",
        "stop": "mysql.server stop"
      },
      "timeout": 2
    },
    {
      "name": "kafka-broker",
      "enable": true,
      "script": {
        "path": "~/Tools/kafka",
        "start": "bin/kafka-storage.sh format --config ./config/kraft/server.properties --cluster-id $(./bin/kafka-storage.sh random-uuid)",
        "stop": "kill -9 $(lsof -t -i :9092)"
      },
      "timeout": 2
    },
    {
      "name": "kafka-queue",
      "enable": true,
      "script": {
        "path": "~/Tools/kafka",
        "start": "./bin/kafka-server-start.sh ./config/kraft/server.properties",
        "stop": "kill -9 $(lsof -t -i :9092)"
      },
      "timeout": 2
    },
    {
      "name": "kafka-connector",
      "enable": true,
      "script": {
        "path": "~/Tools/kafka",
        "start": "./bin/connect-distributed.sh ./config/connect-distributed.properties",
        "stop": "kill -9 $(lsof -t -i :8083)"
      },
      "timeout": 2
    }
  ],
  "services": [
    {
      "name": "tenant-services",
      "enable": true,
      "envars": {
      },
      "ports": [
        8101,
        8102,
        8103
      ],
      "script": {
        "path": "~/Projects/reva/java/tenant-services",
        "start": "sh run.sh"
      },
      "timeout": 5
    },
    {
      "name": "identity-services",
      "enable": true,
      "envars": {
        "LOGGING_LEVEL_AI_REVA": "DEBUG",
        "LOGGING_LEVEL_ORG_SPRINGFRAMEWORK_WEB": "DEBUG",
        "AWS_SESSION_TOKEN": "{{cred_AWS_SESSION_TOKEN}}"
      },
      "ports": [
        8020
      ],
      "script": {
        "path": "~/Projects/reva/java/identity-services",
        "start": "sh run.sh"
      },
      "timeout": 5
    },
    {
      "name": "ui-monorepo",
      "enable": true,
      "envars": {
      },
      "ports": [
        8000,
        8001
      ],
      "script": {
        "path": "~/Projects/reva/node/ui-monorepo",
        "start": "npm run dev"
      },
      "timeout": 5
    },
    {
      "name": "nginx",
      "enable": true,
      "envars": {
      },
      "ports": [
        80,
        443
      ],
      "script": {
        "path": "~/Projects/reva/config/nginx",
        "start": "sh update.sh"
      },
      "timeout": 5
    }
  ],
  "access": {
    "enable": true,
    "timeout": 1,
    "url": "https://reva.local"
  }
}