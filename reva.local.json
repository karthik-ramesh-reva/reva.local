    "DOMAIN_SERVICE_URL": "http://localhost:8010",
    "WORKFLOW_SERVICE_URL": "http://localhost:8016",
    "CEDAR_POLICY_ENGINE_URL": "http://localhost:8070",
    "AVP_POLICY_ENGINE_URL": "http://localhost:8071",
